        <Label Content="Trajanje" HorizontalAlignment="Left" Margin="48,101,0,0" VerticalAlignment="Top"/>
        <Label Content="Zanr" HorizontalAlignment="Left" Margin="48,155,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="123,53,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="259" Name="textBoxNaziv" />
        <TextBox HorizontalAlignment="Left" Height="26" Margin="123,101,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="259" Name="textBoxTrajanje" />
        <TextBox HorizontalAlignment="Left" Height="26" Margin="123,155,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="259" Name="textBoxZanr" />
        <Button Content="Prihvati" HorizontalAlignment="Left" Margin="48,223,0,0" VerticalAlignment="Top" Width="75" Name="buttonPrihvati" Click="buttonPrihvati_Click" />
        <Button Content="Odustani" HorizontalAlignment="Left" Margin="153,223,0,0" VerticalAlignment="Top" Width="75" Name="buttonOdustani" Click="buttonOdustani_Click" />

    </Grid>
</Window>
